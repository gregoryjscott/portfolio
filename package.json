{
  "name": "www",
  "version": "1.0.0",
  "description": "Manages gregoryjscott.com",
  "scripts": {
    "bootstrap": "gem install bundler jekyll && bundle update",
    "tsc": "tsc --noEmit",
    "lint": "npm run tsc && eslint .",
    "lint-fix": "eslint . --fix",
    "types": "typesync --dry",
    "types-fix": "typesync",
    "prep": "npm run generate-desc && npm run fix-links && npm run set-year-range && npm run embed-resources ",
    "fix-links": "ts-node _scripts/fix-links.ts",
    "embed-resources": "ts-node _scripts/embed-resources.ts",
    "set-year-range": "ts-node _scripts/set-year-range.ts",
    "generate-desc": "ts-node _scripts/generate-desc.ts",
    "wipe-desc": "ts-node _scripts/wipe-desc.ts",
    "www": "bundle exec jekyll serve",
    "open": "open http://127.0.0.1:4000",
    "build": "bundle exec jekyll build --verbose",
    "deploy": "npm run build && gh-pages --dist _site --src '**' --repo https://github.com/gregoryjscott/gregoryjscott.github.io.git --branch master --message 'Generated by gregoryjscott/portfolio'",
    "write-resume-pdf": "npm run build && ts-node _scripts/write-resume-pdf.ts"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.10",
    "@types/eslint-config-prettier": "~6.11.3",
    "@types/eslint-plugin-markdown": "~2.0.2",
    "@types/eslint-plugin-prettier": "~3.1.3",
    "@types/gh-pages": "~3.2.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.11",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-markdown": "^3.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "gh-pages": "^3.2.3",
    "gray-matter": "^4.0.3",
    "lodash": "^4.17.21",
    "openai": "^5.0.1",
    "prettier": "2.8.0",
    "puppeteer": "^24.16.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "typesync": "^0.14.3",
    "yaml": "^2.1.3"
  },
  "packageManager": "npm@10.8.2+sha512.c7f0088c520a46596b85c6f8f1da943400199748a0f7ea8cb8df75469668dc26f6fb3ba26df87e2884a5ebe91557292d0f3db7d0929cdb4f14910c3032ac81fb"
}
